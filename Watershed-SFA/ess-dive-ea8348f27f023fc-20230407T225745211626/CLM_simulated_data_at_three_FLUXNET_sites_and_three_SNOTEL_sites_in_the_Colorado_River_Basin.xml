lisher>    <methods>      <methodStep>        <description>          <para>Community Land Models (CLM) were developed for calculating ET at both FLUXNET sites and SNOTEL sites in the Colorado River Basin (Tran et al., 2019), using meteorological forcing datasets obtained from FLUXNET and SNOTEL databases.</para>        </description>      </methodStep>    </methods>    <project id="024b867d-25e3-4025-b68c-6598aded7aa2" scope="system" system="ess-dive">      <title>Watershed Function SFA</title>      <personnel>        <individualName>          <givenName>Eoin</givenName>          <surName>Brodie</surName>        </individualName>        <organizationName>Lawrence Berkeley National Lab</organizationName>        <electronicMailAddress>elbrodie@lbl.gov</electronicMailAddress>        <role>Principal Investigator</role>      </personnel>    </project>    <otherEntity id="ess-dive-06b6d5d64e80c27-20210607T053300186">      <entityName>US_NR1.csv</entityName>      <entityType>text/csv</entityType>    </otherEntity>    <otherEntity id="ess-dive-5af502067ee3462-20210607T053300150">      <entityName>ER_BT.csv</entityName>      <entityType>text/csv</entityType>    </otherEntity>    <otherEntity id="ess-dive-61545300919da05-20210607T053300172">      <entityName>ER_SP.csv</entityName>      <entityType>text/csv</entityType>    </otherEntity>    <otherEntity id="ess-dive-761024162d904fb-20210607T053300193">      <entityName>US_VCM.csv</entityName>      <entityType>text/csv</entityType>    </otherEntity>    <otherEntity id="ess-dive-c0696b8a48e7076-20210607T053300166">      <entityName>ER_PK.csv</entityName>      <entityType>text/csv</entityType>    </otherEntity>    <otherEntity id="ess-dive-7e925ad737cfdaf-20210607T053300180">      <entityName>US_GLE.csv</entityName>      <entityType>text/csv</entityType>    </otherEntity>  </dataset></eml:eml>